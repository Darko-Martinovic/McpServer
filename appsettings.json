{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft": "Warning",
      "System": "Warning",
      "ModelContextProtocol": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=SupermarketDB;Integrated Security=true;TrustServerCertificate=true;"
  },
  "ApplicationMode": {
    "Mode": "webapi",
    "Web": {
      "Enabled": true,
      "Urls": ["http://localhost:5000", "https://localhost:5001"],
      "EnableSwagger": true,
      "EnableCors": true,
      "HttpPort": 5000,
      "HttpsPort": 5001
    },
    "Console": {
      "Enabled": true,
      "Transport": "stdio",
      "VerboseLogging": false
    }
  },
  "AzureSearch": {
    "Endpoint": "",
    "ApiKey": "",
    "IndexName": "mcp-tools"
  },
  "AzureOpenAI": {
    "Endpoint": "",
    "ApiKey": "",
    "ChatCompletionDeploymentName": "",
    "EmbeddingDeploymentName": ""
  }
}
