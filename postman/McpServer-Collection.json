  "item": [
    {
      "name": "Tool Proxy",
      "item": [
        {
          "name": "Execute Tool",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              },
              {
                "key": "Accept",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"tool\": \"{{tool_name}}\",\n  \"arguments\": {},\n  \"originalUserInput\": \"Show me articles with ingredients\"\n}"
            },
            "url": {
              "raw": "{{protocol}}://{{host}}:{{port}}/api/{{api_version}}/tool",
              "protocol": "{{protocol}}",
              "host": ["{{host}}"],
              "port": "{{port}}",
              "path": ["api", "{{api_version}}", "tool"]
            },
            "description": "Execute any MCP tool by name. Supports tool routing, parameter extraction, and multi-tool operations."
          }
        },
        {
          "name": "Search Tools",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              },
              {
                "key": "Accept",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"articles with ingredients\"\n}"
            },
            "url": {
              "raw": "{{protocol}}://{{host}}:{{port}}/api/{{api_version}}/search",
              "protocol": "{{protocol}}",
              "host": ["{{host}}"],
              "port": "{{port}}",
              "path": ["api", "{{api_version}}", "search"]
            },
            "description": "Search for tools using Azure Cognitive Search. Returns matching tools based on query."
          }
        },
        {
          "name": "Get Tool Schemas",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Accept",
                "value": "application/json",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{protocol}}://{{host}}:{{port}}/api/{{api_version}}/tools/schema",
              "protocol": "{{protocol}}",
              "host": ["{{host}}"],
              "port": "{{port}}",
              "path": ["api", "{{api_version}}", "tools", "schema"]
            },
            "description": "Get all available tool schemas from all plugins (Supermarket and ThirdApi)"
          }
        },
        {
          "name": "Tool Proxy Health",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Accept",
                "value": "application/json",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{protocol}}://{{host}}:{{port}}/api/{{api_version}}/proxy/health",
              "protocol": "{{protocol}}",
              "host": ["{{host}}"],
              "port": "{{port}}",
              "path": ["api", "{{api_version}}", "proxy", "health"]
            },
            "description": "Check health of the unified tool proxy service"
          }
        }
      ],
      "description": "Unified Tool Proxy - replaces Node.js middleware with native .NET implementation"
    },
