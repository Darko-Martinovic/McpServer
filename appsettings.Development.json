{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft": "Warning",
      "System": "Warning",
      "ModelContextProtocol": "Debug"
    }
  },
  "ApplicationMode": {
    "Web": {
      "EnableSwagger": true,
      "EnableCors": true
    },
    "Console": {
      "VerboseLogging": true
    }
  }
}
